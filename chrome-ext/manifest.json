{
  "manifest_version": 2,
  "name": "seeQuora",
  "version": "1.0",
	"content_scripts": [
	  {
	    "matches": [
	      "https://www.quora.com/"
	    ],
	    "js": ["content.js"]
	  }
	],
	"browser_action": {
    "default_icon": "icon.png"
  },
  "icons": { "16": "icon.png",
           "48": "icon1.png",
          "128": "icon1.png" }
}